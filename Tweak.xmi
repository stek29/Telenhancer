#include "SharedSettings.h"
#include "NeverExplicit.xm"
#include "ShowDebugSettings.xm"

#define LoadIfShould(Group) if (shouldLoad##Group(settings)) %init(Group)

%ctor {
  TelenhancerSettings *settings = [TelenhancerSettings sharedInstance];
  LoadIfShould(NeverExplicit)
  LoadIfShould(ShowDebugSettings);
}
